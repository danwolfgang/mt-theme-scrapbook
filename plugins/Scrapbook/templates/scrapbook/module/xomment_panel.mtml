<mt:XommentPanelComments>
<mt::Include module="Comment Detail">
</mt:XommentPanelComments>
<mt:If name="rpanel" eq="-1">
<div class="comment" id="comment-update" style="display: none;">
    <div class="inner">
        <div class="comment-content">
            <p id="comment-update-message"></p>
            <p class="comment-update-link"><a href="javascript:void(0);" onclick="panelUpdate(<mt::EntryID>);">Update</a></p>
        </div>
    </div>
</div>
</mt:If>
