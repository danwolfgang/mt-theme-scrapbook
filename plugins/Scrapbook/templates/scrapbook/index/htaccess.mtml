order allow,deny
deny from 66.249.68.177
allow from all

Options +FollowSymlinks
RewriteEngine On
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]
RewriteRule ^tag/(.*)$ /cgi-bin/mt/mt-search.cgi?blog_id=1&tag=$1&limit=20 [L]


# These are the error pages.
ErrorDocument 400 /error.php
ErrorDocument 401 /error.php
ErrorDocument 403 /error.php
# ErrorDocument 404 /error404.php
ErrorDocument 404 /cgi-bin/mt/mt.cgi?__mode=404&blog_id=1
ErrorDocument 500 /error.php

# Redirect attempts to view the daily folder (/2005/01/10/, for example) to the monthly
# archives (/2005/01/, for example).
RedirectMatch permanent /([0-9][0-9][0-9][0-9])/([0-9][0-9])/([0-9][0-9])/?$ <mt:BlogURL>$1/$2/

RewriteRule styles/scrapbook/styles.css.php$ <mt:BlogURL>styles/scrapbook/styles.css [L]

