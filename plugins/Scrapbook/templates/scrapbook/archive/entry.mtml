<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
   "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xmlns:fb="http://www.facebook.com/2008/fbml">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=<mt:PublishCharset>" />
    <meta http-equiv="imagetoolbar" content="no" />
    <meta property="fb:app_id" content="35113519158" />
    <title><mt:EntryTitle remove_html="1">, in the <mt:EntryCategories glue=", "><mt:ParentCategories glue=" &#62; "><mt:CategoryLabel remove_html="1"></mt:ParentCategories></mt:EntryCategories> &#62; category, on <mt:BlogName></title>
    <link rel="stylesheet" href="<mt:StaticWebPath>plugins/ActionStreams/css/action-streams.css" type="text/css" />
    <link rel="stylesheet" href="<mt:Link template="styles">" type="text/css" />
    <link rel="alternate" type="application/rss+xml" title="RSS" href="<mt:FeedburnerURL>" />

    <script type="text/javascript" src="<mt:Link template="javascript">"></script>
    <script type="text/javascript" src="<mt:StaticWebPath>jquery/jquery.js"></script>
    <script type="text/javascript" src="<mt:StaticWebPath>jquery/jquery.form.js"></script>
    <script type="text/javascript" src="<mt:PluginStaticWebPath component="scrapbook">js/jquery.lightbox-0.5.pack.js"></script>
    <script type="text/javascript">
    $(function(){
        $.Lightbox.construct({
            show_info: true,
            show_extended_info: true,
            show_linkback: false,
        });
    });
    </script>
    
    <mt:IfCommentsActive>
        <mt:Include module="Xomment Head" />
        <mt:SetVar name="panel_size" value="50"/>
        <mt:SetVar name="panel_static_list" value="-1--3,1"/>
    </mt:IfCommentsActive>
    
    <script type="text/javascript" src="<mt:BlogURL>pollposition.js"></script>

<script type="text/javascript" src="<mt:StaticWebPath>plugins/FacebookCommenters/fbconnect.js"></script>
<script type="text/javascript" src="<mt:StaticWebPath>plugins/FacebookCommenters/signface.js"></script>
<script type="text/javascript">
/* <![CDATA[ */
    /* BEGIN Facebook Connect Info */
    var fb = {
      api_key: '<mt:FacebookApplicationID>',
      xd_receiver_url: '<mt:StaticWebPath>plugins/FacebookCommenters/xd_receiver.html'
    };
    var story = {
      title: '<mt:EntryTitle encode_js="1">',
      url: '<mt:EntryPermalink>',
      excerpt: '<mt:EntryExcerpt encode_js="1">',
      xid: 'entry-<mt:EntryID>',
      blog_name: '<mt:BlogName encode_js="1">',
      blog_url: '<mt:BlogURL encode_js="1">'
    };
    var image;
    function facebook_send_comment(comment) {
      send_comment(fb, story, image, comment);
    }
    $(document).ready( function () {
      if ( window.location.hash && window.location.hash.match( /^#_logout/ ) ) {
        facebook_logout(fb);
        return;
      }
      apply_commenter_data(fb);
    });
    /* END Facebook Connect Info */
/* ]]> */
</script>


</head>

<body id="<mt:BlogTemplateSetID>">

<mt:Include module="Header">

<div id="container">

    <div id="sidebar">
        <mt:WidgetSet name="Left Column">
    </div>

    <div id="content">
        <mt:If tag="SiteWideAdlinks">
        <div class="adlinks">
            <mt:SiteWideAdlinks>
        </div>
        </mt:If>

        <mt:Include module="Entry Detail">

        <mt:Include module="Comments">

        <mt:Ignore> close the "content" div container </mt:Ignore>
    </div>

    <div id="right-sidebar">
        <mt:WidgetSet name="Right Column">
    </div>

</div>

<mt:Include module="Footer">
